<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.brainsoon.solrservice.res.po.Book" table="xhyb_book">
        <id name="bookid" type="java.lang.Long">
            <column name="bookid" />
            <generator class="native" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="title" length="120"/>
        </property>
        <property name="price" type="java.lang.String">
            <column name="price" length="120"/>
        </property>
        <property name="attribute" type="java.lang.Integer">
            <column name="attribute" length="1"/>
        </property>
    </class>
</hibernate-mapping>
